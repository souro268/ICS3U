count = int(input("Enter count: "))
count += 1 # sets cound to more than one so the for loop is correct for the user and so its not 0 which would break the factorial part
triangular  = 0 
num = 1
print("J |   Tri |  Factorial")
#print("%d %5d %10d" % ("J","b","c"))

for x in range(1, count): 
    triangular  = triangular + x # add x to triangular number which will increace for every interaction
    num = num * x # adds x to factorial number which will be multiplyed for each interation
    print("%d | %5d | %10d" %(x,n,num)) # prints the output

